{
  "name": "infinispan-console",
  "version": "0.7.1",
  "description": "The console for Infinispan/Red Hat Datagrid",
  "main": "index.js",
  "repository": "https://github.com/infinispan/infinispan-console.git",
  "homepage": "http://locgstalhost:11222/console/",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/victory": "^33.1.4",
    "@types/webpack": "^4.41.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^7.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.0",
    "react": "^16.11.0",
    "react-axe": "^3.4.1",
    "react-docgen-typescript-loader": "^3.2.1",
    "react-dom": "^16.11.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^4.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4",
    "license-reporter": "1.2.2"
  },
  "dependencies": {
    "@patternfly/react-catalog-view-extension": "^4.4.8",
    "@patternfly/react-charts": "^6.5.4",
    "@patternfly/react-core": "^4.23.1",
    "@patternfly/react-icons": "^4.4.2",
    "@patternfly/react-styles": "^4.4.2",
    "@patternfly/react-table": "^4.8.6",
    "@patternfly/react-tokens": "^4.5.2",
    "express": "^4.13.4",
    "keycloak-js": "^10.0.2",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1",
    "react-syntax-highlighter": "^12.2.1"
  }
}
