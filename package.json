{
  "name": "infinispan-console",
  "version": "0.14.1",
  "description": "The console for Infinispan/Red Hat Datagrid",
  "main": "index.js",
  "repository": "https://github.com/infinispan/infinispan-console.git",
  "homepage": "http://localhost:11222/console/",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "type-check": "tsc --noEmit",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "deploy-console-to-server": "cp -r ./dist/* ./server/infinispan-server/static/console",
    "cy:run": "cypress run --headless",
    "cy:e2e": "cypress open"
  },
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  },
  "devDependencies": {
    "cypress": "^6.0.1",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/jest": "^26.0.16",
    "@types/react-router-dom": "^5.1.6",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@webpack-cli/serve": "^1.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.14.0",
    "eslint-plugin-i18n-json": "^3.1.0",
    "eslint-plugin-jest-dom": "^3.6.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "imagemin": "^7.0.1",
    "jest": "^26.6.3",
    "license-reporter": "1.3.0",
    "mini-css-extract-plugin": "^1.3.3",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.4.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "16.9.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.5",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.0.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@patternfly/react-catalog-view-extension": "^4.10.2",
    "@patternfly/react-charts": "^6.14.2",
    "@patternfly/react-code-editor": "^4.1.25",
    "@patternfly/react-core": "^4.97.2",
    "@patternfly/react-icons": "^4.9.2",
    "@patternfly/react-styles": "^4.8.2",
    "@patternfly/react-table": "^4.23.2",
    "@patternfly/react-tokens": "^4.10.2",
    "express": "^4.17.1",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "keycloak-js": "^12.0.2",
    "numeral": "^2.0.6",
    "monaco-editor": "^0.21.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.4",
    "react-monaco-editor": "^0.41.2",
    "react-router-last-location": "^2.0.1",
    "react-syntax-highlighter": "^15.4.3",
    "sirv-cli": "^1.0.10"
  }
}
