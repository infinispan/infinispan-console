{
  "distributed-cache": {
    "mode": "SYNC",
    "encoding": {
      "key": {
        "media-type": "application/x-protostream"
      },
      "value": {
        "media-type": "application/x-protostream"
      }
    },
    "memory": {
      "storage": "HEAP"
    },
    "indexing": {
      "indexed-entities": [
        "com.redhat.PlayerScore"
      ],
      "enabled": true
    },
    "statistics": true
  }
}
